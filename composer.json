{
    "name": "hurah/data-types",
    "description": "A collection of classes that represent specific types of data often encountered when coding.",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Boutkam",
            "email": "anton@nui-boutkam.nl"
        }
    ],
    "require": {
        "symfony/finder": "^5.2",
        "php" : "^7.4 || 8.0.*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-bcmath": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.3.8"
    },
    "autoload" : {
        "psr-4" : {
            "Hurah\\Types\\" : "src/Types"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Hurah\\Types\\": "tests/Types"
        }
    }
}
